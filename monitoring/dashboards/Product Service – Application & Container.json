{
  "annotations": { "list": [] },
  "editable": true,
  "panels": [
    {
      "type": "timeseries",
      "title": "App Response Time (p95)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(product_service_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95 Response Time",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "App Error Rate",
      "targets": [
        {
          "expr": "rate(product_service_request_errors_total[5m])",
          "legendFormat": "Errors/sec",
          "refId": "B"
        }
      ],
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "App Throughput (Requests/sec)",
      "targets": [
        {
          "expr": "rate(product_service_requests_total[1m])",
          "legendFormat": "Requests/sec",
          "refId": "C"
        }
      ],
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "App Availability (Uptime)",
      "targets": [
        {
          "expr": "up{job=\"product-service\"}",
          "legendFormat": "Uptime",
          "refId": "D"
        }
      ],
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "App Network Latency (avg)",
      "targets": [
        {
          "expr": "rate(product_service_request_latency_seconds_sum[5m]) / rate(product_service_request_latency_seconds_count[5m])",
          "legendFormat": "Latency (s)",
          "refId": "E"
        }
      ],
      "gridPos": { "x": 0, "y": 16, "w": 24, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "Container CPU Usage",
      "targets": [
        {
          "expr": "rate(container_cpu_usage_seconds_total{container_label_com_docker_compose_service=\"product-service\"}[1m])",
          "legendFormat": "CPU",
          "refId": "F"
        }
      ],
      "gridPos": { "x": 0, "y": 24, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "Container Memory Usage",
      "targets": [
        {
          "expr": "container_memory_usage_bytes{container_label_com_docker_compose_service=\"product-service\"}",
          "legendFormat": "Memory",
          "refId": "G"
        }
      ],
      "gridPos": { "x": 12, "y": 24, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "Container Network Usage",
      "targets": [
        {
          "expr": "rate(container_network_receive_bytes_total{container_label_com_docker_compose_service=\"product-service\"}[1m])",
          "legendFormat": "RX",
          "refId": "H"
        },
        {
          "expr": "rate(container_network_transmit_bytes_total{container_label_com_docker_compose_service=\"product-service\"}[1m])",
          "legendFormat": "TX",
          "refId": "I"
        }
      ],
      "gridPos": { "x": 0, "y": 32, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "Container Disk I/O",
      "targets": [
        {
          "expr": "rate(container_fs_reads_bytes_total{container_label_com_docker_compose_service=\"product-service\"}[1m])",
          "legendFormat": "Read",
          "refId": "J"
        },
        {
          "expr": "rate(container_fs_writes_bytes_total{container_label_com_docker_compose_service=\"product-service\"}[1m])",
          "legendFormat": "Write",
          "refId": "K"
        }
      ],
      "gridPos": { "x": 12, "y": 32, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "Container Availability (Uptime)",
      "targets": [
        {
          "expr": "up{container_label_com_docker_compose_service=\"product-service\"}",
          "legendFormat": "Uptime",
          "refId": "L"
        }
      ],
      "gridPos": { "x": 0, "y": 40, "w": 24, "h": 8 }
    }
  ],
